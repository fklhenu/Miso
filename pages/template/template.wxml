<template name="tempItem">
  <view class="temp-container">
    <view class="temp-author-date">
      <image class="temp-author" src="{{avatar}}"></image>
      <text class="temp-date">{{date}}</text>
    </view>
    <text class="temp-title">{{title}}</text>
    <image class="temp-image" src="{{imgSrc}}"></image>
    <text class="temp-content">{{content}}</text>
    <view class="temp-like">
      <image class="temp-like-image" src="/images/icon/chat.png"></image>
      <text class="temp-like-font">{{collection}}</text>
      <image class="temp-like-image" src="/images/icon/view.png"></image>
      <text class="temp-like-font">{{reading}}</text>
    </view>
  </view>
</template>